Cell["\<\
Definimos constantes a1, a2, a3, a4 para evaluar la esperanza y la desviaci\
\[OAcute]n est\[AAcute]ndar de odds ratio:\
\>", "Text",
 CellChangeTimes->{{3.816639497255973*^9, 
  3.816639560991954*^9}},ExpressionUUID->"2d7a97ad-ba5f-468d-8fae-\
f5158c435147"],

Cell[BoxData[{
 RowBox[{"a1", ":=", 
  RowBox[{"435", "+", 
   RowBox[{"1", "/", "2"}]}]}], "\[IndentingNewLine]", 
 RowBox[{"a2", ":=", 
  RowBox[{"147", "+", 
   RowBox[{"1", "/", "2"}]}]}], "\[IndentingNewLine]", 
 RowBox[{"a3", ":=", 
  RowBox[{"375", "+", 
   RowBox[{"1", "/", "2"}]}]}], "\[IndentingNewLine]", 
 RowBox[{"a4", ":=", 
  RowBox[{"134", "+", 
   RowBox[{"1", "/", "2"}]}]}], "\[IndentingNewLine]", 
 RowBox[{"B", ":=", 
  RowBox[{
   RowBox[{"Gamma", "[", "a1", "]"}], "*", 
   RowBox[{"Gamma", "[", "a2", "]"}], "*", 
   RowBox[{"Gamma", "[", "a3", "]"}], "*", 
   RowBox[{"Gamma", "[", "a4", "]"}]}]}], "\[IndentingNewLine]", 
 RowBox[{"B1", ":=", 
  RowBox[{"1", "/", "B"}]}]}], "Input",
 CellChangeTimes->{{3.81663956553386*^9, 3.8166396326297393`*^9}},
 CellLabel->"In[47]:=",ExpressionUUID->"876d0d94-4265-48e5-ac06-efb714ccccf4"],

Cell["Calculamos la esperanza de U=odds ratio ", "Text",
 CellChangeTimes->{{3.8166396443089685`*^9, 
  3.8166396622655897`*^9}},ExpressionUUID->"c0ec743c-9a20-4957-b68e-\
0c61f84746a1"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{"EU", ":=", 
  RowBox[{"N", "[", 
   RowBox[{
    RowBox[{"Integrate", "[", 
     RowBox[{
      RowBox[{"B1", "*", 
       SuperscriptBox["u1", "a1"], "*", 
       SuperscriptBox["u2", 
        RowBox[{"a1", "+", "a2", "-", "1"}]], "*", 
       SuperscriptBox["u3", 
        RowBox[{"a3", "+", "a4", "-", "1"}]], "*", 
       SuperscriptBox["u4", 
        RowBox[{"a2", "+", "a4", "-", "1"}]], "*", 
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{
         RowBox[{
          RowBox[{"-", "u1"}], "*", "u2"}], "-", 
         RowBox[{"u2", "*", "u3"}], "-", "u3", "-", 
         RowBox[{"u3", "*", "u4"}]}]]}], ",", 
      RowBox[{"{", 
       RowBox[{"u1", ",", "0", ",", "\[Infinity]"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"u2", ",", "0", ",", "\[Infinity]"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"u3", ",", "0", ",", "\[Infinity]"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"u4", ",", "0", ",", "\[Infinity]"}], "}"}]}], "]"}], ",", 
    "10"}], "]"}]}], "\[IndentingNewLine]", "EU"}], "Input",
 CellChangeTimes->{{3.8166395973154783`*^9, 3.8166395981439133`*^9}, {
  3.8166396747127013`*^9, 3.8166396836239996`*^9}},
 CellLabel->"In[53]:=",ExpressionUUID->"eddddee4-e4fa-4fa3-a94c-dce0020bfde1"],

