Cell[BoxData[
 RowBox[{"\[IndentingNewLine]", 
  RowBox[{"Integrate", "[", 
   RowBox[{
    RowBox[{
     SuperscriptBox["y", 
      RowBox[{"(", 
       RowBox[{"\[Nu]", "/", "2"}], ")"}]], "*", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"y", "-", 
        RowBox[{"x", "*", "y"}]}], ")"}], 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"\[Mu]", "/", "2"}], ")"}], "-", "1"}]], "*", 
     SuperscriptBox["\[ExponentialE]", 
      RowBox[{
       RowBox[{"-", "y"}], "/", "2"}]]}], ",", 
    RowBox[{"{", 
     RowBox[{"y", ",", "0", ",", "\[Infinity]"}], "}"}], ",", " ", 
    RowBox[{"Assumptions", "\[Rule]", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"\[Mu]", "\[Element]", "Integers"}], "&&", 
       RowBox[{"\[Nu]", "\[Element]", "Integers"}], "&&", 
       RowBox[{"\[Mu]", ">", "0"}], "&&", 
       RowBox[{"\[Nu]", ">", "1"}], "&&", 
       RowBox[{"x", " ", "\[Epsilon]", " ", "Reals"}], " ", "&&", " ", 
       RowBox[{"x", ">", "1"}]}], "}"}]}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.8166400791805105`*^9, 3.816640090079076*^9}},
 CellLabel->"In[60]:=",ExpressionUUID->"9bd990da-b248-459e-9686-93ffad4dcbe3"],

